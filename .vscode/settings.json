{
  "eslint.packageManager": "yarn",
  "eslint.format.enable": true,
  "eslint.workingDirectories": [
    "packages/native-client",
    "packages/native-common"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
